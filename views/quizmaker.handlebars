<header>
    <button class="hss">
        <p class="score">Quit</p>
    </button>
    <div class="questCreatedSet" id="qcSet">

    </div>
    <button class="time">
        <p class="timer">Seconds</p>
    </button>
    <button class="hss">
        <p class="score">Quiz: {{quiz.name}}</p>
    </button>
</header>

<button class="startBtn">
    <p>Start</p>
</button>

<div class="questContain">
   
    <input class="question qcFields" placeholder="Write your question..." aria-placeholder="Write question...."
        id="writtenQuest"></input>


    <div class="answerBtnCon">
        <label for="answerchoicea">
            <p>Answer Choice A</p>:
        </label>
        <input class="answerBtn qcFields" id="answerchoicea"></input>
        <label for="answerchoiceb">
            <p>Answer Choice B</p>:
        </label>
        <input class="answerBtn qcFields" id="answerchoiceb"></input>
        <label for="answerchoicec">
            <p>Answer Choice C</p>:
        </label>
        <input class="answerBtn qcFields" id="answerchoicec"></input>
        <label for="answerchoiced">
            <p>Answer Choice D</p>:
        </label>
        <input class="answerBtn qcFields" id="answerchoiced"></input>
        <label for="correctR">Choose the correct answer:</label>
        <select id="correctR" name="correctR">
            <option value="">--Select--</option>
            <option value="1">Answer Choice A</option>
            <option value="2">Answer Choice B</option>
            <option value="3">Answer Choice C</option>
            <option value="4">Answer Choice D</option>
        </select>


        <button class="saveQuest">Add</button>
        <button class="finishQuiz">Finish</button>
    </div>
</div>


<div class="endScreen">
    <h2 class="finalScore"></h2>
    <div class="HSCon">
        <p>Questions Added:</p>
        <ol id="draftlist"></ol>
        <p>
            <button class="playAgain">See My Quiz</button>
        </p>
    </div>
</div>


<script src="/js/quizmaker.js"></script>